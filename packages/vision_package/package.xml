<?xml version="1.0"?>
<package format="2">
  <name>vision_package</name>
  <version>0.0.1</version>
  <description>The vision_package for Duckiebot object detection with bounding boxes</description>

  <maintainer email="andylazcano@gmail.com">Andy Lazcano</maintainer>
  <license>None</license>

  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies for building and running the package -->
  <build_depend>rospy</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>cv_bridge</build_depend>
  <build_depend>std_msgs</build_depend>

  <exec_depend>rospy</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>cv_bridge</exec_depend>
  <exec_depend>std_msgs</exec_depend>

  <export>
    <!-- Other tools can request additional information be placed here -->
  </export>
</package>

